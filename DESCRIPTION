Package: naturaList
Type: Package
Title: Classify occurrence records by confidence levels in the species identification
Version: 0.1.0
Authors@R: c(
    person("Arthur", "Rodrigues", "Vinicius", email = "rodrigues.arthur.v@gmail.com", role = "cre"),
